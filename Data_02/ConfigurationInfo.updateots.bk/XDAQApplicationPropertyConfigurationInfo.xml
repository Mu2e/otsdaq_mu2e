<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<ROOT xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ConfigurationInfo.xsd">
		<CONFIGURATION Name="XDAQApplicationPropertyConfiguration">
			<VIEW Name="XDAQ_APPLICATION_PROPERTY_CONFIGURATION" Type="File,Database,DatabaseTest" Description="Properties%20entered%20in%20this%20table%20will%20be%20included%20in%20the%20xdaq%20configuration%20file%20as%20an%20XML%20property%20tag.%0A%0AThere%20is%20one%20exception%20to%20this%20rule%2C%20if%20PropertyType%20is%20'ots-only%20Property'%20then%20it%20will%20be%20omitted%20from%20the%20xdaq%20configuration%20file%2C%20and%20will%20be%20used%20within%20the%20otsdaq%20Supervisor%20class%20as%20a%20Supervisor%20property.%20Known%20such%20ots-only%20properties%20are%20listed%20as%20fixed%20choice%20options%20within%20the%20PropertyName%20field.">
				<COLUMN Type="GroupID-PropertyGroup" 	 Name="PropertyGroupID" 	 StorageName="PROPERTY_GROUP_ID" 		DataType="VARCHAR2" 		DataChoices=""/>
				<COLUMN Type="UID" 	 Name="UID" 	 StorageName="UID" 		DataType="VARCHAR2" 		DataChoices=""/>
				<COLUMN Type="OnOff" 	 Name="Status" 	 StorageName="STATUS" 		DataType="VARCHAR2" 		DataChoices=""/>
				<COLUMN Type="FixedChoiceData" 	 Name="PropertyName" 	 StorageName="PROPERTY_NAME" 		DataType="VARCHAR2" 		DataChoices="arbitraryBool=1,UserPermissionsThreshold,UserGroupsAllowed,UserGroupsDisallowed,RequireUserLockRequestTypes,CheckUserLockRequestTypes,AutomatedRequestTypes,AllowNoLoginRequestTypes"/>
				<COLUMN Type="FixedChoiceData" 	 Name="PropertyType" 	 StorageName="PROPERTY_TYPE" 		DataType="VARCHAR2" 		DataChoices="arbitraryBool=1,xsd%3Astring,xsd%3Ainteger,xsd%3AunsignedInt,xsd%3Aboolean,xsd%3AunsignedShort,ots%3A%3ASupervisorProperty"/>
				<COLUMN Type="Data" 	 Name="PropertyValue" 	 StorageName="PROPERTY_VALUE" 		DataType="VARCHAR2" 		DataChoices=""/>
				<COLUMN Type="Comment" 	 Name="CommentDescription" 	 StorageName="COMMENT_DESCRIPTION" 		DataType="VARCHAR2" 		DataChoices=""/>
				<COLUMN Type="Author" 	 Name="Author" 	 StorageName="AUTHOR" 		DataType="VARCHAR2" 		DataChoices=""/>
				<COLUMN Type="Timestamp" 	 Name="RecordInsertionTime" 	 StorageName="RECORD_INSERTION_TIME" 		DataType="TIMESTAMP WITH TIMEZONE" 		DataChoices=""/>
			</VIEW>
		</CONFIGURATION>
	</ROOT>
